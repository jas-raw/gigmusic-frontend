                    "notas": "Am;E7;E7;Eb7;D7",
                    "espacio": "0;15;12;1;1",
                    "letra": " En la vida conocí mujer igual a la flaca"
                },
                {
                    "notas": "C;E;Am",
                    "espacio": "21;10;10",
                    "letra": "Coral negro de La Habana,tremendísima mulata"
                },
                {
                    "notas": "E;E7;Eb7;D7",
                    "espacio": "23;15;1;1",
                    "letra": "Cien libras de piel y hueso,cuarenta kilos de salsa"
                },
                {
                    "notas": "F7;E;F;F#;G",
                    "espacio": "20;1;16;2;2",
                    "letra": "Y en la cara dos soles que sin palabras hablan"
                },
